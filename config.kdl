// Example OTP deployment configuration
// This file demonstrates how to configure deployments and test suites

deployment name="Sound Transit Production" url="https://otp.soundtransit.org" {
  test-suite name="Sound Transit Core Tests" class="SoundTransit" interval="5m"
}

deployment name="Hope Link Staging" url="https://staging.hopelink.org/otp" {
  test-suite name="Hope Link Basic" class="HopeLink" interval="10m"
}
